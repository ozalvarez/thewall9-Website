@model thewall9.data.binding.PageWeb
<section id="services">
    <header class="intro-header" style="background-image: url('@Html.FindValue(Model, "header")')">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
                    <div class="page-heading">
                        <h1>@Html.FindValue(Model, "title")</h1>
                        <hr class="small">
                        <span class="subheading">@Html.FindValue(Model, "subtitle")</span>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <div class="container">
        <div class="row">
            @{int _Cont = 0;}
            @foreach (var item in Html.FindItems(Model, "list-service"))
            {
                if (item.Enabled)
                {
                    <div class="col-md-4">
                        <h2 class="text-center">
                            @Html.FindValue(item, "title")
                        </h2>
                        @foreach (var item2 in Html.FindItems(item, "list"))
                        {
                            <div class="text-center ">
                                @Html.GetValue(item2)
                            </div>
                        }
                    </div>
                    if (_Cont % 3 == 2)
                    {
                        <div class="clearfix"></div>
    <hr />
                    }
                    _Cont++;
                }
            }
        </div>
    </div>
    <a href="@Html.FindValue(Model, "contact-btn-url")" class="btn btn-xl btn-default btn-on btn-block top10">
        <span class="glyphicon glyphicon-envelope small"></span> @Html.FindValue(Model, "contact-btn-text")
    </a>
</section>
